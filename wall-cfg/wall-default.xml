<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wall>
    <name>default</name>
    <configuration>
        <connections>
            <connection>
                <name>Build</name>
                <url>http://build.visuwall.ci</url>
                <pluginName>Demo Build Plugin</pluginName>
                <include-projects/>
                <include-metrics/>
            </connection>
            <connection>
                <name>Track</name>
                <url>http://track.visuwall.ci</url>
                <pluginName>Demo Track Plugin</pluginName>
                <include-projects/>
                <include-metrics/>
            </connection>
            <connection>
                <name>Analysis</name>
                <url>http://analysis.visuwall.ci</url>
                <pluginName>Demo Analysis Plugin</pluginName>
                <include-projects/>
                <include-metrics>
                    <metric-key>loc</metric-key>
                </include-metrics>
            </connection>
        </connections>
    </configuration>
</wall>